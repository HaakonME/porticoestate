<?xml version="1.0" encoding="UTF-8" ?>
<rss version="0.91">
	<channel>
		<title>{title}</title>
		<link>{link}</link>
		<description>
			<![CDATA[{description}]]>
		</description>
		<language>{lang}</language> 
		
		<image>
			<title>{img_title}</title>
			<link>{link}</link>
			<url>{img_url}</url>
			<description>
				<![CDATA[{description}]]>
			</description>
		</image>

		<!-- BEGIN item -->
		<item>
			<title>{subject}</title>
			<link>{item_link}</link>
			<description><![CDATA[{teaser}]]></description>
		</item>
		<!-- END item -->
	</channel>
</rss>

